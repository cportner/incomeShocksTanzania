	Journal = {Criminology},
	Keywords = {Tanzania},
	Number = {4},
	Pages = {859--866},
	Publisher = {Blackwell Publishing Ltd},
	Title = {Using the Correct Statistical Test for the Equality of Regression Coefficients},
	Url = {http://dx.doi.org/10.1111/j.1745-9125.1998.tb01268.x},
	Volume = {36},
	Year = {1998},
	Bdsk-File-1 = {},
